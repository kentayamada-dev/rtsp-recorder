{
  "name": "rtsp-recorder",
  "version": "0.0.7",
  "main": "out/main/index.js",
  "author": {
    "name": "Kenta Yamada"
  },
  "description": "RTSP Recorder",
  "homepage": "https://github.com/kentayamada-dev/rtsp-recorder",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kentayamada-dev/rtsp-recorder.git"
  },
  "scripts": {
    "format": "prettier . --write --config prettier.config.ts",
    "postinstall": "electron-builder install-app-deps",
    "dev": "electron-vite dev",
    "typecheck": "pnpm typecheck:node && pnpm typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "build": "pnpm typecheck && electron-vite build",
    "build:prev": "pnpm build && electron-builder --dir",
    "build:prod": "pnpm build && electron-builder"
  },
  "dependencies": {
    "electron-updater": "6.6.2"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "2.0.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@fontsource/inter": "5.2.8",
    "@mui/material": "7.3.6",
    "@types/node": "25.0.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "babel-plugin-react-compiler": "1.0.0",
    "electron": "39.2.7",
    "electron-builder": "26.0.12",
    "electron-log": "5.4.3",
    "electron-vite": "5.0.0",
    "prettier": "3.7.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "typescript": "5.9.3",
    "vite": "npm:rolldown-vite@7.2.11"
  },
  "engines": {
    "node": "25.2.1",
    "pnpm": "10.26.0"
  },
  "volta": {
    "node": "25.2.1",
    "pnpm": "10.26.0"
  }
}
